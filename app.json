{
  "expo": {
    "name": "horus",
    "description": "Secure chat.",
    "slug": "horus",
    "privacy": "public",
    "sdkVersion": "29.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "ios": {
      "bundleIdentifier": "com.mvaskiv.horus",
      "supportsTablet": true
    },
    "android": {
      "package": "com.mvaskiv.horus",
      "googleServicesFile": "./google-services.json"
    }
  }
}
